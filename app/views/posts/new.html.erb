<h1>投稿</h1>

<%= form_with model: @post, local: true do |f| %>
    <div class="form-group">
        <label>内容</label>
        <%= f.text_field :content, class: "form-control" %>
    </div>
    <%= link_to "戻る", posts_path, class: "btn btn-secondary" %>
    <%= f.submit "登録", class: "btn btn-primary"%>

<% end %>

